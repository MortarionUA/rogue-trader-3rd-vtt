<div class="rt-panel spacer rt-grid-col-2">
    <h1 class="rt-panel__header">Crew</h1>
    <div class="rt-field__wrapper">
        <span class="rt-field__header">Crew Rating</span>
        <input type="number" data-dtype="Number" class="rt-field__input" name="system.crewRating" value="{{actor.crewRating}}" />
    </div>
    <div class="rt-field__wrapper">
        <span class="rt-field__header">Crew Limit</span>
        <input type="number" data-dtype="Number" class="rt-field__input" name="system.crew.max" value="{{actor.crew.max}}" />
    </div>
    <div class="rt-field__wrapper">
        <span class="rt-field__header">Crew Left</span>
        <input type="number" data-dtype="Number" class="rt-field__input" name="system.crew.value" value="{{actor.crew.value}}" />
    </div>
    <div class="rt-field__wrapper">
        <span class="rt-field__header">Morale Limit</span>
        <input type="number" data-dtype="Number" class="rt-field__input" name="system.morale.max" value="{{actor.morale.max}}" />
    </div>
    <div class="rt-field__wrapper">
        <span class="rt-field__header">Morale Left</span>
        <input type="number" data-dtype="Number" class="rt-field__input" name="system.morale.value" value="{{actor.morale.value}}" />
    </div>
</div>
